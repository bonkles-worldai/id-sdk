{
  "name": "id-sdk",
  "version": "3.0.0-pre.0",
  "description": "Map editing made easy",
  "repository": "ideditor/id-sdk",
  "license": "ISC",
  "contributors": [
    "Bryan Housel <bryan@mapbox.com>"
  ],
  "keywords": [
    "map",
    "editor"
  ],
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "@types/d3-polygon": "^1.0.7",
    "@types/geojson": "^7946.0.7",
    "@types/node": "^12.6.0",
    "babel-jest": "^24.8.0",
    "docdash": "^1.1.1",
    "jest": "^24.8.0",
    "jest-extended": "^0.11.2",
    "jsdoc": "^3.6.0",
    "lerna": "^3.15.0",
    "prettier": "^1.18.2",
    "shx": "^0.3.0",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "clean": "lerna run clean --parallel",
    "build": "lerna run build --parallel",
    "buildpretty": "npm run build && prettier --write \"./packages/**/*.{html,css,ts,js}\"",
    "docs": "rm -f docs/*.html && jsdoc -c ./jsdoc.config.js packages && prettier --write \"./docs/**/*.{html,css,ts,js}\"",
    "test": "jest",
    "prettier": "prettier --write \"./**/*.{html,css,ts,js}\""
  },
  "engines": {
    "node": ">=8.15.0",
    "npm": ">=5.0.0"
  }
}
