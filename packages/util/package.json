{
  "name": "@id-sdk/util",
  "version": "3.0.0-pre.5",
  "description": "util",
  "homepage": "https://github.com/ideditor/id-sdk/tree/main/packages/util#readme",
  "repository": "github:ideditor/id-sdk",
  "license": "ISC",
  "contributors": [
    "Ben Clark <clarkben@fb.com> (https://github.com/bonkles)",
    "Bryan Housel <bhousel@gmail.com> (https://github.com/bhousel)",
    "Danny Krause (https://github.com/vanreece)",
    "Milos Brzakovic <mbrzakovic@microsoft.com> (https://github.com/mbrzakovic)"
  ],
  "files": ["built/", "src/"],
  "type": "module",
  "source": "./src/index.js",
  "types": "./built/util.d.ts",
  "main": "./built/util.cjs",
  "module": "./built/util.mjs",
  "scripts": {
    "clean": "shx rm -rf ./built",
    "build": "run-p build:**",
    "build:cjs": "esbuild ./src/index.js --platform=node --format=cjs --bundle --external:'@id-sdk/*' --external:'aes-js' --outfile=./built/util.cjs",
    "build:esm": "esbuild ./src/index.js --platform=neutral --format=esm --bundle --external:'@id-sdk/*' --external:'aes-js' --outfile=./built/util.mjs",
    "build:types": "tsc",
    "esbuild": "run-p build:cjs build:esm",
    "test": "jest --config=../../package.json --roots=./packages/util"
  },
  "dependencies": {
    "aes-js": "^3.1.2",
    "diacritics": "1.3.0",
    "@id-sdk/extent": "^3.0.0-pre.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
