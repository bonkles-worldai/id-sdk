{
  "name": "@id-sdk/math",
  "version": "3.0.0-pre.5",
  "description": "Collection of iD-sdk math libraries",
  "homepage": "https://github.com/ideditor/id-sdk/tree/main/packages/math#readme",
  "repository": "github:ideditor/id-sdk",
  "license": "ISC",
  "contributors": [
    "Bryan Housel <bhousel@gmail.com> (https://github.com/bhousel)"
  ],
  "files": ["math.ts", "built/"],
  "type": "module",
  "source": "./math.ts",
  "types": "./built/math.d.ts",
  "main": "./built/math.cjs.js",
  "module": "./built/math.esm.js",
  "scripts": {
    "clean": "shx rm -rf ./built",
    "build": "run-p build:**",
    "build:cjs": "esbuild ./math.ts --platform=node --format=cjs --outfile=./built/math.cjs.js",
    "build:esm": "esbuild ./math.ts --platform=neutral --format=esm --outfile=./built/math.esm.js",
    "build:types": "tsc",
    "esbuild": "run-p build:cjs build:esm",
    "test": "jest --config=../../package.json --roots=./packages/math"
  },
  "dependencies": {
    "@id-sdk/extent": "^3.0.0-pre.5",
    "@id-sdk/geo": "^3.0.0-pre.5",
    "@id-sdk/geom": "^3.0.0-pre.5",
    "@id-sdk/projection": "^3.0.0-pre.5",
    "@id-sdk/tiler": "^3.0.0-pre.5",
    "@id-sdk/vector": "^3.0.0-pre.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
