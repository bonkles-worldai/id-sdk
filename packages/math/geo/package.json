{
  "name": "@id-sdk/geo",
  "version": "3.0.0-pre.5",
  "description": "Geographic (spherical) math functions",
  "homepage": "https://github.com/ideditor/id-sdk/tree/main/packages/math/geo#readme",
  "repository": "github:ideditor/id-sdk",
  "license": "ISC",
  "contributors": [
    "Bryan Housel <bhousel@gmail.com> (https://github.com/bhousel)"
  ],
  "files": ["built/", "src/"],
  "type": "module",
  "source": "./src/geo.ts",
  "types": "./built/geo.d.ts",
  "main": "./built/geo.cjs.js",
  "module": "./built/geo.esm.js",
  "scripts": {
    "clean": "shx rm -rf ./built",
    "build": "run-p build:**",
    "build:cjs": "esbuild ./src/geo.ts --platform=node --format=cjs --outfile=./built/geo.cjs.js",
    "build:esm": "esbuild ./src/geo.ts --platform=neutral --format=esm --outfile=./built/geo.esm.js",
    "test": "jest --config ../../../jest.config.js --roots=./packages/math/geo",
    "tsc": "tsc"
  },
  "dependencies": {
    "@id-sdk/vector": "^3.0.0-pre.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
