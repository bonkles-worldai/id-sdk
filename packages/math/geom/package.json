{
  "name": "@id-sdk/geom",
  "version": "3.0.0-pre.5",
  "description": "Geometric (planar) math functions",
  "homepage": "https://github.com/ideditor/id-sdk/tree/main/packages/math/geom#readme",
  "repository": "github:ideditor/id-sdk",
  "license": "ISC",
  "contributors": [
    "Bryan Housel <bhousel@gmail.com> (https://github.com/bhousel)"
  ],
  "files": ["built/", "src/"],
  "type": "module",
  "source": "./src/geom.ts",
  "types": "./built/geom.d.ts",
  "main": "./built/geom.cjs.js",
  "module": "./built/geom.esm.js",
  "scripts": {
    "clean": "shx rm -rf ./built",
    "build": "run-p build:**",
    "build:cjs": "esbuild ./src/geom.ts --platform=node --format=cjs --outfile=./built/geom.cjs.js",
    "build:esm": "esbuild ./src/geom.ts --platform=neutral --format=esm --outfile=./built/geom.esm.js",
    "build:types": "tsc",
    "esbuild": "run-p build:cjs build:esm",
    "test": "jest --config=../../../package.json --roots=./packages/math/geom"
  },
  "dependencies": {
    "@id-sdk/extent": "^3.0.0-pre.5",
    "@id-sdk/vector": "^3.0.0-pre.5",
    "d3-polygon": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
