{
  "name": "@id-sdk/vector",
  "version": "3.0.0-pre.5",
  "description": "Vector (coordinate) math functions",
  "homepage": "https://github.com/ideditor/id-sdk/tree/main/packages/math/vector#readme",
  "repository": "github:ideditor/id-sdk",
  "license": "ISC",
  "contributors": [
    "Bryan Housel <bhousel@gmail.com> (https://github.com/bhousel)"
  ],
  "files": ["built/", "src/"],
  "type": "module",
  "source": "./src/vector.ts",
  "types": "./built/vector.d.ts",
  "main": "./built/vector.cjs.js",
  "module": "./built/vector.esm.js",
  "scripts": {
    "clean": "shx rm -rf ./built",
    "build": "run-p build:**",
    "build:cjs": "esbuild ./src/vector.ts --platform=node --format=cjs --outfile=./built/vector.cjs.js",
    "build:esm": "esbuild ./src/vector.ts --platform=neutral --format=esm --outfile=./built/vector.esm.js",
    "build:types": "tsc",
    "esbuild": "run-p build:cjs build:esm",
    "test": "jest --config=../../../package.json --roots=./packages/math/vector"
  },
  "publishConfig": {
    "access": "public"
  }
}
