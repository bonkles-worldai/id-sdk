{
  "name": "@id-sdk/projection",
  "version": "3.0.0-pre.5",
  "description": "Projection class for converting between Lon/Lat (λ,φ) and Cartesian (x,y) coordinates",
  "homepage": "https://github.com/ideditor/id-sdk/tree/main/packages/math/projection#readme",
  "repository": "github:ideditor/id-sdk",
  "license": "ISC",
  "contributors": [
    "Bryan Housel <bhousel@gmail.com> (https://github.com/bhousel)"
  ],
  "files": ["built/", "src/"],
  "type": "module",
  "source": "./src/projection.ts",
  "types": "./built/projection.d.ts",
  "main": "./built/projection.cjs.js",
  "module": "./built/projection.esm.js",
  "scripts": {
    "clean": "shx rm -rf ./built",
    "build": "run-p build:**",
    "build:cjs": "esbuild ./src/projection.ts --platform=node --format=cjs --outfile=./built/projection.cjs.js",
    "build:esm": "esbuild ./src/projection.ts --platform=neutral --format=esm --outfile=./built/projection.esm.js",
    "test": "jest --config ../../../jest.config.js --roots=./packages/math/projection",
    "tsc": "tsc"
  },
  "dependencies": {
    "@id-sdk/vector": "^3.0.0-pre.5",
    "d3-geo": "^2.0.1",
    "d3-zoom": "^2.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
